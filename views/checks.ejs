<link rel="stylesheet" href="/stylesheets/style.css" />
<body></body>
<div class="bg-container"></div>
<img src="/images/Logo.png"width="120"/>
<form method="get" action="/auth/logout">
    <button type="submit">Logout</button>
  </form>
<h1>Checks</h1>
<h2>Hello <%= user %>!</h2>
<div id="profile-button"><form method="GET" action="/profile">
    <button type="submit">Go Back to Profile</button>
  </form>
  </div>

  <ul id="checkintool">
    <% allChecks.forEach((checkin) => { %>
      <% const date = new Date(checkin.createdAt) %>
    <li>
            <p> <%= user %> has made  <%- checkin.eventName %> on <%= date.toLocaleString('en-US', { timeZone: 'UTC', month: 'short', day: 'numeric', year: 'numeric', hour: 'numeric', minute: 'numeric', hour12: false }) %> </p>
    </li>
   <% }) %>
</ul>
